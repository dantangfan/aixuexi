{
    "manifest_version": 2,
    "name": "爱学习",
    "version": "1.0",
    "description": "好好学习，天天向上",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png"
        },
        "default_title": "爱学习",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "tabs"
    ]
}
